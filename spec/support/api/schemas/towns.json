{
  "$schema": "https://json-schema.org/draft-04/schema#",
  "title": "Product",
  "description": "A product list",
  "type": "object",
  "properties": {
    "count": {
      "type": "number",
      "description": "Product counter"
    },
    "rows": {
      "type": "array",
      "items": {
        "type": "object",
        "description": "Product data",
        "properties": {
          "id": {
            "type": "number",
            "description": "Product id"
          }
        },
        "required": ["id"]
      }
    }
  },
  "required": ["count", "rows"]
}
