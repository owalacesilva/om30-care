{
  "$schema": "https://json-schema.org/draft-04/schema#",
  "title": "Resident",
  "description": "A resident list",
  "type": "object",
  "properties": {
    "count": {
      "type": "number",
      "description": "Resident counter"
    },
    "rows": {
      "type": "array",
      "items": {
        "type": "object",
        "description": "Resident data",
        "properties": {
          "id": {
            "type": "number",
            "description": "Resident id"
          }
        },
        "required": ["id"]
      }
    }
  },
  "required": ["count", "rows"]
}
